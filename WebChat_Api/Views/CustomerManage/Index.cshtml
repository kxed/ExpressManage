@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no">
    <meta name="format-detection" content="telephone=no, email=no">
    <title>百世汇通快递</title>
    <script src="~/Content/js/libs/jquery-2.0.3.min.js"></script>
    <link href="~/Content/bootstrap-3.3.5-dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/bootstrap-3.3.5-dist/js/bootstrap.min.js"></script>
    <link href="~/Content/css/default.css" rel="stylesheet" media="screen" charset="utf-8" />
    <link href="~/Content/css/address.css" rel="stylesheet" media="screen" charset="utf-8" />
    <link href="~/Content/css/city-picker.css" rel="stylesheet" media="screen" charset="utf-8" />
    <script src="~/Content/js/libs/city-picker.data.js"></script>
    <script src="~/Content/js/common/city-picker.js"></script>
</head>
<body class="myaddress-book">
    <input type="hidden" value="@ViewBag.CustomerType" id="hdcustomerType" />
    <header class="page-title">
        <h1 id="titName" class="title">我的地址簿</h1>
    </header>
    <section class="form-main">
        <input type="search" maxlength="12" placeholder="请输入姓名/电话信息">
    </section>
    <ul id="addresslistid" class="list"></ul>
    <footer>
        <button id="createBtn" class="confirm-single-btn fixed-100" type="button">新增地址</button>
    </footer>

    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Content/js/common/common.core.js"></script>
    <script src="~/Content/js/common/units.js"></script>
    <script src="~/Content/js/common/map.js"></script>
    <link href="~/Content/css/swiper.min.css" rel="stylesheet" />
    <script src="~/Content/js/libs/swiper.min.js"></script>
    <script src="~/Content/js/order/my-address-book.js"></script>
</body>
</html>
