@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no">
    <meta name="format-detection" content="telephone=no, email=no">
    <title>百世汇通快递</title>
    <script src="~/Content/js/libs/jquery-2.0.3.min.js"></script>
    <link href="~/Content/bootstrap-3.3.5-dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/bootstrap-3.3.5-dist/js/bootstrap.min.js"></script>
    <link href="~/Content/css/swiper.min.css" rel="stylesheet" />
    <link href="~/Content/css/default.css" rel="stylesheet" media="screen" charset="utf-8" />
    <link href="~/Content/css/order-query.css" rel="stylesheet" />
</head>
<body class="page-query member-login-page">
    <header class="page-title">
        <h1 id="titName" class="title">我的订单</h1>
    </header>
    <section class="form-main hide">
        <input type="search" maxlength="12" placeholder="请输入姓名/电话信息">
    </section>
    <section class="my-express">
        <ul id="my-sent" class="list content"></ul>
    </section>
    <footer>
        <button id="createBtn" class="confirm-single-btn fixed-100 hide" type="button">新增地址</button>
    </footer>
    <!-- 模态框（Modal） -->
    <div class="modal fade" id="cancelReasonModal" tabindex="-1" role="dialog" aria-labelledby="cancelReasonModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" style="z-index:9999;" data-dismiss="modal" aria-hidden="true">
                        ×
                    </button>
                    <h4 class="modal-title" id="cancelReasonModalLabel">
                        取消原因
                    </h4>
                </div>
                <div class="modal-body page-cancel">
                    <dl class="reason">
                        <dt class="center">
                            <h2>预约已取消  请告诉我们原因</h2>
                            <p>我们会努力为您提供更好的服务</p>
                            <p class="tip-three hide">请选择取消原因</p>
                        </dt>
                        <dd class="select">计划有变，暂时不需要寄件了</dd>
                        <dd class="select">赶时间，更改为其它寄件方式</dd>
                        <dd class="select">预约信息有误，需要修改</dd>
                        <dd class="select">误发预约</dd>
                        <dd class="select">其他</dd>
                    </dl>
                </div>
                <div class="modal-footer">
                    <button type="button" id="btnSubmitReason" class="btn btn-danger btn-lg btn-block">
                        确定
                    </button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
    <!-- 模态框（Modal）end -->
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Content/js/common/common.core.js"></script>
    <script src="~/Content/js/common/units.js"></script>
    <script src="~/Content/js/common/map.js"></script>
    
    <script src="~/Content/js/libs/swiper.min.js"></script>
    <script src="~/Content/js/order/order-query.js"></script>
</body>
</html>
